<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="StaticLib_easy" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option extended_obj_names="1" />
		<Build>
			<Target title="Debug">
				<Option output="..\lib\Debug\libStaticLib_easy" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="..\obj\Debug" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="..\lib\Release\libStaticLib_easy" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="..\obj\Release" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="..\doc\StaticLibDocs.txt">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="..\include\StaticLib.h" />
		<Unit filename="..\src\StaticLib.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
