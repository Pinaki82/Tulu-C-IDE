%c -Wundef
%c -Wredundant-decls
%c -Wfloat-equal
%c -Wmain
%c -Wunreachable-code
%c -Wshadow
%c -Wcast-align
%c -Wswitch-default
%c -Wswitch-enum
%c -Wstrict-prototypes
%c -Wmissing-prototypes
%c -Wmissing-declarations
%c -Wnested-externs
%c -Wformat=2
%c -Wformat-security
%c -Wformat-signedness
%c -Wformat-y2k
%c -Wconversion
%c -Weverything
%c -ferror-limit=0
%c -ftemplate-backtrace-limit=0

%c --sysroot=$HOME/.arduino15/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/
