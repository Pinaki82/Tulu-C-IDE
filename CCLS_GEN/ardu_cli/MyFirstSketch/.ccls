%c -std=c11
%cpp -std=c++17
%cpp -stdlib=libc++
%cpp -stdlib=libstdc++
%h %hpp --include=Global.h
-Iinc
-DMACRO


%c -Wall
%c -Weffc++
%c -Wextra
%c -pedantic
%c -Wfatal-errors
%c -Winit-self
%c -Wnon-virtual-dtor
